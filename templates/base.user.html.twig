			<script src="{{ asset("asset/pages/widget/amchart/light.js") }}"></script>
			<script src="{{ asset("asset/pages/widget/amchart/pie.min.js") }}"></script>
			<script src="https://www.amcharts.com/lib/3/plugins/export/export.min.js"></script>
			<!-- menu js -->
			<script src="{{ asset("asset/js/pcoded.min.js") }}"></script>
